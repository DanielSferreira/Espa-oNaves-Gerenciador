@model List<StarWarsManageShip.Models.PilotoNavesView>
@{
    ViewData["Title"] = "Listar Viagens";
}
<table class="table">
    <thead>
        <tr>
            <th scope="col">ID</th>
            <th scope="col">Pessoa</th>
            <th scope="col">Nave</th>
            <th scope="col">Destino</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <th scope="row">@item.Id</th>
                <td> @item.piloto</td>
                <td>@item.nave</td>
                <td>@item.planeta</td>
            </tr>
        }
    </tbody>
</table>